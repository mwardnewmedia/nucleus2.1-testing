                <a href="{{ block_3.settings.video_button_2 }}" class="video-highlight-btn" {% if block_3.settings.new_window_2 == true %}target="_blank"{% endif %} style="{% if block_3.settings.video_button_2_type == 'solid' %}background:{{ block_3.settings.button_2_color }};color:#FFF;border:3px solid {{ block_3.settings.button_2_color }};{% else %}color:{{ block_3.settings.button_2_color }};border:3px solid {{ block_3.settings.button_2_color }}{% endif %}">
                  {{ block_3.settings.video_button_text_2 }}
                </a>
                {% endif %}
              </div>
              {% endif %}
      		</div>
        </div>
    <div class="grid-item small--one-whole medium--one-half large--one-half">
            <div class="video-highlights">
              {% if block_4.settings.video_header != blank %}
              	<h2 class="text-center video-highlights-header">{{ block_4.settings.video_header }}</h2>
              {% endif %}  
              {% if block_4.settings.video_description != blank %}
              	<p class="text-center">{{ block_4.settings.video_description }}</p>
              {% endif %}  
              <div class="video-wrapper">
                {% if block_4.settings.video_url == blank %}
                  <iframe src="//www.youtube.com/embed/_9VUPq3SxOc?rel=0&showinfo=0&vq=720" width="850" height="480" frameborder="0" allowfullscreen></iframe>
                {% else %}
                  {% if block_4.settings.video_url.type == "youtube" %}
                    <iframe src="//www.youtube.com/embed/{{ block_4.settings.video_url.id }}?rel=0&showinfo=0&vq=720" width="100%" height="480" frameborder="0" allowfullscreen></iframe>
                  {% endif %}
                  {% if block_4.settings.video_url.type == "vimeo" %}
                    <iframe src="//player.vimeo.com/video/{{ block_4.settings.video_url.id }}?byline=0&portrait=0&badge=0" width="100%" height="480" frameborder="0" allowfullscreen></iframe>
                  {% endif %}
                {% endif %}
              </div>
              {% if block_4.settings.video_button_text_1 != blank %}
              <div class="video-buttons">
                <a href="{{ block_4.settings.video_button_1 }}" class="video-highlight-btn" {% if block_4.settings.new_window == true %}target="_blank"{% endif %} style="{% if block_4.settings.video_button_1_type == 'solid' %}background:{{ block_4.settings.button_1_color }};color:#FFF;border:3px solid {{ block_4.settings.button_1_color }};{% else %}color:{{ block_4.settings.button_1_color }};border:3px solid {{ block_4.settings.button_1_color }}{% endif %}">
                  {{ block_4.settings.video_button_text_1 }}
                </a>
                {% if block_4.settings.video_button_text_2 != blank %}
                <a href="{{ block_4.settings.video_button_2 }}" class="video-highlight-btn" {% if block_4.settings.new_window_2 == true %}target="_blank"{% endif %} style="{% if block_4.settings.video_button_2_type == 'solid' %}background:{{ block_4.settings.button_2_color }};color:#FFF;border:3px solid {{ block_4.settings.button_2_color }};{% else %}color:{{ block_4.settings.button_2_color }};border:3px solid {{ block_4.settings.button_2_color }}{% endif %}">
                  {{ block_4.settings.video_button_text_2 }}
                </a>
              {% endif %}
              </div>
              {% endif %}
      		</div>
        </div>
    {% endif %}
  </div>
</div>
</div>


