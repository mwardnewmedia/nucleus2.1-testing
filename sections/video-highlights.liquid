            <div class="video-highlights">
              {% if block_1.settings.video_header != blank %}
              	<h2 class="text-center">{{ block_1.settings.video_header }}</h2>
              {% endif %}  
              {% if block_1.settings.video_description != blank %}
              	<p class="text-center">{{ block_1.settings.video_description }}</p>
              {% endif %}  
              <div class="video-wrapper">
                {% if block_1.settings.video_url == blank %}
                  <iframe src="//www.youtube.com/embed/_9VUPq3SxOc?rel=0&showinfo=0&vq=720" width="850" height="480" frameborder="0" allowfullscreen></iframe>
                {% else %}
                  {% if block_1.settings.video_url.type == "youtube" %}
                    <iframe src="//www.youtube.com/embed/{{ block_1.settings.video_url.id }}?rel=0&showinfo=0&vq=720" width="100%" height="480" frameborder="0" allowfullscreen></iframe>
                  {% endif %}
