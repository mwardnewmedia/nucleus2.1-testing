                  {{ 'layout.customer.sign_in' | t | customer_login_link }}
                {% endif %}
              </span>
     </div>
    {% endif %}
    {% endif %}
  {% endunless %}

{% assign now = "now" | date: "%s" %}<!--Announcement Bar Scheduling-->
{% assign show_at = section.settings.show_at | date: "%s" %}<!--Announcement Bar Scheduling-->
{% assign remove_at = section.settings.remove_at | date: "%s" %}<!--Announcement Bar Scheduling-->

  {% if show_at == blank or now > show_at %}<!--Announcement Bar Scheduling-->
    {% if remove_at == blank or now < remove_at %}<!--Announcement Bar Scheduling--> 
      <div class="notification-bar" style="background-color:{{ section.settings.announcement_background_color }};">
        {% if section.settings.show_announcement %}
                <div class="site-header--text-links{% if section.settings.announcement_text == blank %} medium-down--hide{% endif %}">
                  {% if section.settings.show_announcement %}
                    {% if section.settings.announcement_link != blank %}
                      <a style="color:{{ section.settings.announcement_text_color }};" href="{{ section.settings.announcement_link }}">
                    {% endif %}
      
                      <p style="color:{{ section.settings.announcement_text_color }};">{{ section.settings.announcement_text | escape }}</p>
      
                    {% if section.settings.announcement_link != blank %}
                      </a>
                    {% endif %}
                  {% endif %}
                 
                </div>
        {% endif %}
      </div>
    {% endif %}<!--Announcement bar scheduling-->
  {% endif %}<!--Announcement bar scheduling--> 

