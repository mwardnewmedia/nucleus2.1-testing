<header class="site-header" role="banner" data-section-id="{{ section.id }}" data-section-type="header-section">
  <div class="wrapper">
    
    {% if section.settings.logo_alignment == "logo_left"  %}

    <div class="grid--full">
      <div class="grid-item large--one-half">
        {% if request.page_type == 'index' %}
          <h1 class="header-logo" itemscope itemtype="http://schema.org/Organization">
        {% else %}
          <div class="h1 header-logo" itemscope itemtype="http://schema.org/Organization">
        {% endif %}
          {% if section.settings.logo %}
          {% capture image_size %}{{ section.settings.logo_max_height | escape }}x{% endcapture %}
          {%- assign img_url = section.settings.logo | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          {% capture logo_alt %}{{ section.settings.logo.alt | default: shop.name }}{% endcapture %}
          <a href="{{ routes.root_url }}" itemprop="url">
            <div class="lazyload__image-wrapper no-js header-logo__image" style="max-height:{{ section.settings.logo_max_height }}px;">
                <img class="lazyload js"
                  data-src="{{ img_url }}"
                  data-widths="[1080, 1296, 1512, 1728, 2048]"
                  data-aspectratio="{{ section.settings.logo.aspect_ratio }}"
                  data-sizes="auto"
                  alt="{{ logo_alt | escape }}"
                  style="height:{{ section.settings.logo_max_height }}px;">
            </div>
            <noscript>
              {% capture image_size %}{{ section.settings.logo_max_height | escape }}x{% endcapture %}
              <img src="{{ section.settings.logo | img_url: image_size }}"
                srcset="{{ section.settings.logo | img_url: image_size }} 1x, {{ section.settings.logo | img_url: image_size, scale: 2 }} 2x"
                alt="{{ logo_alt | escape }}"
                itemprop="logo"
                style="max-height:{{ section.settings.logo_max_height }}px;">
            </noscript>
          </a>
          <!--- Sponsor Logo --->
          {% if section.settings.sponsor_logo %}
          {% capture image_size %}{{ section.settings.sponsor_logo_max_height | escape }}x{% endcapture %}
          {%- assign img_url = section.settings.sponsor_logo | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          {% capture sponsor_logo_alt %}{{ section.settings.sponsor_logo.alt | default: shop.name }}{% endcapture %}
          <a href="{{ section.settings.sponsor_link }}" itemprop="url" style="margin-left:10px;" class="medium-down--hide">
            <div class="lazyload__image-wrapper no-js header-logo__image" style="max-height:{{ section.settings.sponsor_logo_max_height }}px;">
                <img class="lazyload js"
                  data-src="{{ img_url }}"
                  data-widths="[1080, 1296, 1512, 1728, 2048]"
                  data-aspectratio="{{ section.settings.sponsor_logo.aspect_ratio }}"
                  data-sizes="auto"
                  alt="{{ sponsor_logo_alt | escape }}"
                  style="height:{{ section.settings.sponsor_logo_max_height }}px;">
            </div>
            <noscript>
              {% capture image_size %}{{ section.settings.sponsor_logo_max_height | escape }}x{% endcapture %}
              <img src="{{ section.settings.sponsor_logo | img_url: image_size }}"
                srcset="{{ section.settings.sponsor_logo | img_url: image_size }} 1x, {{ section.settings.sponsor_logo | img_url: image_size, scale: 2 }} 2x"
                alt="{{ sponsor_logo_alt | escape }}"
                itemprop="logo"
                style="max-height:{{ section.settingssponsor_logo_max_height }}px;">
            </noscript>
          </a>
          {% endif %}  
          {% else %}
            <a href="{{ routes.root_url }}" itemprop="url">{{ shop.name }}</a>
          {% endif %}
        {% if request.page_type == 'index' %}
          </h1>
        {% else %}
          </div>
        {% endif %}
      </div>

      <div class="grid-item medium-down--hide large--one-half text-center large--text-right">
        <div class="site-header--text-links{% if section.settings.announcement_text == blank %} medium-down--hide{% endif %}">
          <span style="display: flex; align-items: center;">
            {% unless section.settings.hide_customer_login %}
              <i style="font-size: 24px;" class="fa-solid fa-circle-user"></i>
              {% if shop.customer_accounts_enabled %}
                <span class="site-header--meta-links medium-down--hide">
                {% if customer %}
                 {% capture first_name %}{% endcapture %}
                    <a style="display:block;" href="{{ routes.account_url }}">Hi, {{ customer.first_name }}</a> 
                  {% else %}
                    {{ 'layout.customer.sign_in' | t | customer_login_link }}
                    <span class="site-header--spacer">{{ 'layout.customer.or' | t }}</span>
                    <span>{{ 'layout.customer.create_account' | t | customer_register_link }}</span>
                {% endif %}    
                </span>
              {% endif %}
            {% endunless %}      
          </span>
        </div>
          
          <br class="medium-down--hide">

        {% unless section.settings.hide_shopping_links%}
        <a href="{{ routes.cart_url }}" class="header-cart-btn cart-toggle" aria-label="shopping cart">
          <span class="icon icon-cart" style="color: #fff"></span>
          {{ 'layout.cart.cart' | t }} <span class="cart-count cart-badge--desktop {% if cart.item_count == 0 %}hidden-count{% endif %}">{{ cart.item_count }}</span>
        </a>
        {% endunless %}

        {% unless section.settings.hide_search_bar %}
          {% include 'search-bar' %}
		{% endunless %}
      </div>
    </div>
    
    {% elsif section.settings.logo_alignment == "logo_center" %}
    
    <div class="logo-center">
      <div class="logo-center-wrap">
        {% if request.page_type == 'index' %}
          <h1 class="header-logo text-center" itemscope itemtype="http://schema.org/Organization">
        {% else %}
          <div class="h1 header-logo text-center" itemscope itemtype="http://schema.org/Organization">
        {% endif %}
          {% if section.settings.logo %}
          {% capture image_size %}{{ section.settings.logo_max_height | escape }}x{% endcapture %}
          {%- assign img_url = section.settings.logo | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          {% capture logo_alt %}{{ section.settings.logo.alt | default: shop.name }}{% endcapture %}
          <a href="{{ routes.root_url }}" itemprop="url">
            <div class="lazyload__image-wrapper no-js header-logo__image" style="max-height:{{ section.settings.logo_max_height }}px;">
                <img class="lazyload js"
                  data-src="{{ img_url }}"
                  data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                  data-aspectratio="{{ section.settings.logo.aspect_ratio }}"
                  data-sizes="auto"
                  alt="{{ logo_alt | escape }}"
                  style="height:{{ section.settings.logo_max_height }}px;">
            </div>
            <noscript>
              {% capture image_size %}{{ section.settings.logo_max_height | escape }}x{% endcapture %}
              <img src="{{ section.settings.logo | img_url: image_size }}"
                srcset="{{ section.settings.logo | img_url: image_size }} 1x, {{ section.settings.logo | img_url: image_size, scale: 2 }} 2x"
                alt="{{ logo_alt | escape }}"
                itemprop="logo"
                style="max-height:{{ section.settings.logo_max_height }}px;">
            </noscript>
          </a>
          <!--- Sponsor Logo --->
          {% if section.settings.sponsor_logo %}
          {% capture image_size %}{{ section.settings.sponsor_logo_max_height | escape }}x{% endcapture %}
          {%- assign img_url = section.settings.sponsor_logo | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
          {% capture sponsor_logo_alt %}{{ section.settings.sponsor_logo.alt | default: shop.name }}{% endcapture %}
          <a href="{{ section.settings.sponsor_link }}" itemprop="url" style="margin-left:10px;" class="medium-down--hide">
            <div class="lazyload__image-wrapper no-js header-logo__image" style="max-height:{{ section.settings.sponsor_logo_max_height }}px;">
                <img class="lazyload js"
                  data-src="{{ img_url }}"
                  data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                  data-aspectratio="{{ section.settings.sponsor_logo.aspect_ratio }}"
                  data-sizes="auto"
                  alt="{{ sponsor_logo_alt | escape }}"
                  style="height:{{ section.settings.sponsor_logo_max_height }}px;">
            </div>
            <noscript>
              {% capture image_size %}{{ section.settings.sponsor_logo_max_height | escape }}x{% endcapture %}
              <img src="{{ section.settings.sponsor_logo | img_url: image_size }}"
                srcset="{{ section.settings.sponsor_logo | img_url: image_size }} 1x, {{ section.settings.sponsor_logo | img_url: image_size, scale: 2 }} 2x"
                alt="{{ sponsor_logo_alt | escape }}"
                itemprop="logo"
                style="max-height:{{ section.settingssponsor_logo_max_height }}px;">
            </noscript>
          </a>
          {% endif %}  
          {% else %}
            <a href="{{ routes.root_url }}" itemprop="url">{{ shop.name }}</a>
          {% endif %}
        {% if request.page_type == 'index' %}
          </h1>
        {% else %}
          </div>
        {% endif %}
      </div>

        <div class="text-center search-center">
          <div class="site-header--text-links{% if section.settings.announcement_text == blank %} medium-down--hide{% endif %}">
            <span style="display: flex; align-items: center;">
              {% unless section.settings.hide_customer_login %}
                <i style="font-size: 24px;" class="fa-solid fa-circle-user"></i>
              {% if shop.customer_accounts_enabled %}
                <span class="site-header--meta-links medium-down--hide">
              {% if customer %}
                {% capture first_name %}<a href="{{ routes.account_url }}">{{ customer.first_name }}</a>{% endcapture %}
                {{ 'layout.customer.logged_in_as_html' | t: first_name: first_name }} &middot; {{ 'layout.customer.log_out' | t | customer_logout_link }}
              {% else %}
                {{ 'layout.customer.sign_in' | t | customer_login_link }}
                <span class="site-header--spacer">{{ 'layout.customer.or' | t }}</span>
                <span style="margin-right: 30px;">
                {{ 'layout.customer.create_account' | t | customer_register_link }}
                </span>
              {% endif %}    
              </span>
              {% endif %}
            {% endunless %}
          </span>
        </div>

        <br class="medium-down--hide">

        {% unless section.settings.hide_search_bar %}
        {% include 'search-bar' %}
		{% endunless %}
   
		{% unless section.settings.hide_shopping_links %}
        <a href="{{ routes.cart_url }}" class="header-cart-btn cart-toggle">
          <span class="icon icon-cart"></span>
          {{ 'layout.cart.cart' | t }} <span class="cart-count cart-badge--desktop {% if cart.item_count == 0 %}hidden-count{% endif %}">{{ cart.item_count }}</span>
        </a>
        {% endunless %}
      </div>
    </div>
    
    {% endif %}

  </div>
</header>



<div id="mobileNavBar">
  <div class="display-table-cell">
    <button class="menu-toggle mobileNavBar-link" aria-controls="navBar" aria-expanded="false"><span class="icon icon-hamburger" aria-hidden="true"></span><span class="sr-only">Menu</span></button>
  </div>
  <div class="display-table-cell">
    {% unless section.settings.hide_shopping_links %}
    <a href="{{ routes.cart_url }}" class="cart-toggle mobileNavBar-link">
      <span class="icon icon-cart"></span>
      <span class="sr-only">Cart</span><span class="cart-count {% if cart.item_count == 0 %}hidden-count{% endif %}" style="position: absolute; right: 13px; top: 0px;">{{ cart.item_count }}</span>
    </a>
    {% endunless %}
  </div>
</div>

{% unless section.settings.hide_search_bar %}          
  <div id="ecom-mobile-search">
    <form id="ecom-search-bar" action="/search" method="get" class="search-bar" role="search">
    <input type="hidden" name="type" value="product">

    <input type="search" name="q" value="" placeholder="Search all products..." aria-label="Search all products...">
    <button type="submit" class="search-bar--submit icon-fallback-text">
      <span class="icon icon-search" aria-hidden="true"></span>
      <span class="fallback-text">Search</span>
    </button>
  </form>
  </div> 
{% endunless %} 

<nav class="nav-bar" id="navBar" role="navigation">
  
  <div class="wrapper">
    {% include 'search-bar' %}
    {% include 'mobile-nav' %}
    {% include 'site-nav' %}
  </div>
</nav>

{% endif %}
  
  
{% if section.settings.sticky_header %}

<script>
  
  
  document.getElementById('shopify-section-header').style.display = "block";
  
  document.getElementById('shopify-section-header').classList.add("sticky-header");
  
  let headerHeight = document.getElementById('shopify-section-header').offsetHeight;
  
  document.body.style.marginTop = headerHeight+"px";
  
</script>
  
{% endif %}

{% endif %}
